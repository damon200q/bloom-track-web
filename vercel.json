{
  "version": 2,
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/sitemap.xml",
      "destination": "/sitemap.xml",
      "continue": false
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}